{
  "name": "n8n-nodes-ollama-reranker",
  "version": "1.5.2",
  "description": "Ollama Reranker for n8n - VL Classifier integration + Auto-detect + Dynamic model loading (Vector Store provider + workflow node)",
  "author": "Gabriel BRUMENT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/theseedship/n8n_reranker.git"
  },
  "bugs": {
    "url": "https://github.com/theseedship/n8n_reranker/issues"
  },
  "homepage": "https://github.com/theseedship/n8n_reranker#readme",
  "files": [
    "dist/**/*",
    "src/**/*.svg",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "prepare": "cd .. && husky custom-nodes/.husky"
  },
  "keywords": [
    "n8n",
    "n8n-nodes",
    "n8n-community-node-package",
    "ollama",
    "reranker",
    "vector-store",
    "rag",
    "ai"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/OllamaReranker/OllamaReranker.node.js",
      "dist/nodes/OllamaRerankerWorkflow/OllamaRerankerWorkflow.node.js"
    ],
    "credentials": []
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "form-data": "^4.0.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
